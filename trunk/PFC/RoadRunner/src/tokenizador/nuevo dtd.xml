<?xml version="1.0" encoding="UTF-8"?>
<hierarchy>
	<defaultzone>
		<type>WC_DOCTYPE</type>
		<type>CUSTOM_HTMLCOMMENT</type>
		<zone>CDATA_ZONE</zone>
		<type>CUSTOM_HTMLATT</type>
		<type>CUSTOM_WORD</type>
	</defaultzone>
	
	<zoneentry>
		<zone>CDATA_ZONE</zone>
		<defaultmatch>
			<regexp>
				<![CDATA[(<!\[CDATA\[)(?:(?:(?:[^\]])|(?:\][^\]])|(?:\]\][^>]))*)(\]\]>)]]>
			</regexp>
			<prefix>1</prefix>
			<sufix>2</sufix>
		</defaultmatch>
		<allow>
			<type>CUSTOM_WORD</type>
		</allow>
	</zoneentry>
	
	<typeentry>
		<type>WC_DOCTYPE</type>
		<equals>type</equals>
		<level>1</level>
		<defaultmatch>
			<regexp>
				<![CDATA[<!DOCTYPE[^>]*>]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>CUSTOM_HTMLCOMMENT</type>
		<equals>text</equals>
		<level>1</level>
		<defaultmatch>
			<regexp>
				<![CDATA[<!--((?:(?:[^-])|(?:-[^-])|(?:--[^>]))*)-->]]>
			</regexp>
			<text>
				<![CDATA[[INIT][1][END]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_COMMENT</type>
		</generic>
	</typeentry>
	
		<typeentry>
		<type>WC_COMMENT</type>
		<equals>type</equals>
		<level>2</level>
		<defaultmatch>
			<regexp>
				<![CDATA[.*]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>CUSTOM_WORD</type>
		<equals>text</equals>
		<level>1</level>
		<defaultmatch>
			<regexp>
				<![CDATA[(?:(?:\p{L})+)|(?:\s+)|(?:\d+)|(?:\W)|_]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_NONHTML</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>CUSTOM_HTMLATT</type>
		<equals>text</equals>
		<level>1</level>
		<defaultmatch>
			<regexp>
				<![CDATA[(<)(?:\s*)(?:(/)(?:\s*))?((?:\w|\d|-)+(?:\:(?:\w|\d|-)+)?)((?:(?:\s+)(?:(?:\w||d|-)+(?:\:(?:\w|\d|-)+)?)(?:\s*)\=(?:\s*)(?:(?:"(?:[^"<>]*)")|(?:[^"  \t\n\x0B\f\r<>]+)))*)(?:\s*)(?:(/)(?:\s*))?(>)]]>
			</regexp>
			<text>
				<![CDATA[[INIT][1][2][3][4][5][6][END]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>CUSTOM_HTML</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>CUSTOM_HTML</type>
		<equals>text</equals>
		<level>2</level>
		<defaultmatch>
			<regexp>
				<![CDATA[(<)(?:\s*)(?:(/)(?:\s*))?((?:\w|\d)+(?:\:(?:\w|\d)+)?)((?:(?:\s+)(?:(?:\w|\d)+(?:\:(?:\w|\d)+)?)(?:\s*)\=(?:\s*)(?:(?:"(?:[^"<>]*)")|(?:[^"  \t\n\x0B\f\r<>]+)))*)(?:\s*)(?:(/)(?:\s*))?(>)]]>
			</regexp>
			<text>
				<![CDATA[[INIT][1][2][3][5][6][END]]]>
			</text>	
		</defaultmatch>
		<generic>
			<type>WC_HTML</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_HTML</type>
		<equals>type</equals>
		<level>3</level>
		<defaultmatch>
			<regexp>
				<![CDATA[(<)(?:\s*)(?:(/)(?:\s*))?((?:\w|\d)+(?:\:(?:\w|\d)+)?)((?:(?:\s+)(?:(?:\w|\d)+(?:\:(?:\w|\d)+)?)(?:\s*)\=(?:\s*)(?:(?:"(?:[^"<>]*)")|(?:[^"  \t\n\x0B\f\r<>]+)))*)(?:\s*)(?:(/)(?:\s*))?(>)]]>		</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_NONHTML</type>
		<equals>type</equals>
		<level>2</level>
		<defaultmatch>
			<regexp>
				<![CDATA[[^<> \t\n\x0B\f\r]+]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_PUNCTUATION</type>
			<type>WC_ALPHANUMERIC</type>
			<type>WC_SPACE</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_PUNCTUATION</type>
		<equals>type</equals>
		<level>3</level>
		<defaultmatch>
			<regexp>
				<![CDATA[\W]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_ALPHANUMERIC</type>
		<equals>type</equals>
		<level>3</level>
		<defaultmatch>
			<regexp>
				<![CDATA[\w+]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_NUMERIC</type>
			<type>WC_ALPHABETIC</type>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>
	
	<typeentry>
		<type>WC_SPACE</type>
		<equals>type</equals>
		<level>3</level>
		<defaultmatch>
			<regexp>
				<![CDATA[\s+]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_NUMERIC</type>
			<type>WC_ALPHABETIC</type>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_NUMERIC</type>
		<equals>type</equals>
		<level>4</level>
		<defaultmatch>
			<regexp>
				<![CDATA[\d+]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_ALPHABETIC</type>
		<equals>type</equals>
		<level>4</level>
		<defaultmatch>
			<regexp>
				<![CDATA[[a-zA-Z_]+]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_CAPITALIZED</type>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_CAPITALIZED</type>
		<equals>type</equals>
		<level>5</level>
		<defaultmatch>
			<regexp>
				<![CDATA[[A-Z][a-zA-Z_]*]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_ALLCAPS</type>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_ALLCAPS</type>
		<equals>type</equals>
		<level>6</level>
		<defaultmatch>
			<regexp>
				<![CDATA[[A-Z][A-Z_]+]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
			<type>WC_ANYTHING</type>
		</generic>
	</typeentry>

	<typeentry>
		<type>WC_ANYTHING</type>
		<equals>type</equals>
		<level>7</level>
		<defaultmatch>
			<regexp>
				<![CDATA[.+]]>
			</regexp>
			<text>
				<![CDATA[[ALL]]]>
			</text>
		</defaultmatch>
		<generic>
		</generic>
	</typeentry>
</hierarchy>
